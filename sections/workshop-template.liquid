  <script>
  
    const toggle = (item) => {
      const content = item.querySelector("[x-role=\"menu-content\"]");
      return content.style.display === "block"? "none" : "block"
    };
  
    const changeDisplay = (item, display) =>
      item.querySelector("[x-role=\"menu-content\"]").style.display = display;
  
    const menuButtonClicked = e => {
      const style = toggle(e.target);
      var evt = new Event("menu-click", {});
      document.body.dispatchEvent(evt);
      changeDisplay(e.target, style);
    };
  
    document.querySelectorAll("[x-role=\"menu-button\"]").forEach(x => {
        x.addEventListener("click", menuButtonClicked);
        document.body.addEventListener("menu-click", e => changeDisplay(x, "none"));
      }
    );
  
  </script>

